<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.fragments.Home_Fragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="10dp">

            <Button
                android:id="@+id/homeFragmentLiveExamBtn"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="@drawable/textfield_border"
                android:layout_marginBottom="10dp"
                android:text="Live Exam" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="@drawable/textfield_border"
                android:layout_marginBottom="10dp"
                android:text="Free Weekly Model Test" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/textfield_border"
                android:text="Read Again" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/textfield_border"
                android:text="Chapter wise Question Bank" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/textfield_border"
                android:text="Year wise Question Bank" />


        </LinearLayout>

    </FrameLayout>
</layout>